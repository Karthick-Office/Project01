<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<!-- thread count = how many parallel sessions to run at a time -->

<suite name="testsuite" verbose="1" parallel="classes" thread-count="2">

<test name="TestCycle1">
<classes>
<class name="com.TestNg.Selenium.Paralleltest"></class>
  <class name="com.TestNg.Selenium.Paralleltest1"></class>
  <class name="com.TestNg.Selenium.Paralleltest2"></class>
  <class name="com.TestNg.Selenium.Paralleltest3"></class>
</classes>
</test>

</suite>